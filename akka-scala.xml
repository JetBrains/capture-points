<capture-points>
  <capture-point enabled="false"
                 class-name="akka.actor.ScalaActorRef"
                 method-name="$bang"
                 capture-key-expression="message"/>
  <capture-point enabled="false"
                 class-name="akka.actor.RepointableActorRef"
                 method-name="$bang"
                 capture-key-expression="message"/>
  <capture-point enabled="false"
                 class-name="akka.actor.LocalActorRef"
                 method-name="$bang"
                 capture-key-expression="message"/>
  <capture-point enabled="false"
                 insert-class-name="akka.actor.Actor$class"
                 insert-method-name="aroundReceive"
                 insert-key-expression="msg" />
</capture-points>